{
    "version": 2,
    "name": "defraudai",
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/assets/(.*)",
            "dest": "/assets/$1"
        },
        {
            "src": "/(.*)\\.(.*)$",
            "dest": "/$1.$2"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ],
    "env": {
        "VITE_API_BASE_URL": "@vite_api_base_url",
        "VITE_GEMINI_MODEL_NAME": "@vite_gemini_model_name"
    }
}